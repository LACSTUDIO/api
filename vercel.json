{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/umami-home.js",
        "use": "@vercel/node"
      },
      {
        "src": "pages/**/*.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/umami-home",
        "dest": "pages/api/umami-home.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }